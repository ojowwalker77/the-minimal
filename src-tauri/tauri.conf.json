{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "dMinimal",
  "version": "0.1.0",
  "identifier": "com.dminimal.dminimal",
  "build": {
    "frontendDist": "../src/templates",
    "devUrl": "https://dminimal.com/"
  },
  "app": {
    "windows": [
      {
        "title": "dMinimal",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ]
  }
}
